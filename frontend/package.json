    "eslint": "^8.57.1",
    "flat": "^6.0.1",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "rimraf": "^5.0.10",
    "vite": "^5.4.8",
